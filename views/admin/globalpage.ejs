        }

        .wrapper {
            display: flex;
            align-items: center;
            height: 100vh;
        }

        .right_side {
            width: 25%;
            height: 100%;
            background-color: #ffffff;
            margin-top: auto;
            border-right: 1px solid #e3e0e0;
            position: sticky;
        }

        .left_side {
            width: 75%;
            height: 100%;
            background-color: #fff;
        }

        .profile-img {
            width: 100%;
            height: 100%;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .profile-img img {
            width: 250px;
            height: 250px;
            object-fit: cover;
            border-radius: 1rem;
        }

        .profile-logo {
            width: 100%;
            height: 100%;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .row {
            width: 100%;
            margin-top: 10px;
        }

        .headsection {
            width: 100%;
            height: 100%;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
        }

        .name {
            font-size: 28px;
            font-family: 'Inter', sans-serif;
            font-weight: 600;

        }

        .headline {
            margin-top: 0px;
            font-family: 'Inter', sans-serif;
            font-weight: 500;
            font-size: 18px;
            color: #6d6d6d;
        }

        .buttons-group {
            width: 100%;
            height: 100%;
            display: flex;
            justify-content: space-evenly;
            align-items: center;

            @media (max-width: 768px) {
                .container {
                    padding: 10px;
                    /* Adjust as needed */
                }

                /* Add additional responsive styles here */
            }
        }
    </style>
</head>

